{
  "name": "codebase-visualizer",
  "version": "0.1.0",
  "description": "A macOS application that helps users upload, analyze, and visualize codebases",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "update-cursor": "node scripts/update-cursor.js",
    "update-docs": "node scripts/update-cursor.js && echo 'Documentation updated'",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "build": "electron-builder build --mac --publish never",
    "package-mac": "electron-builder build --mac --publish never",
    "build-docs": "typedoc --out docs src/"
  },
  "keywords": [
    "codebase",
    "visualization",
    "code-analysis",
    "architecture"
  ],
  "author": "Teev-dev",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "electron": "^28.0.0",
    "electron-builder": "^25.1.8",
    "eslint": "^9.23.0",
    "jest": "^29.7.0",
    "typedoc": "^0.28.1",
    "typescript": "^5.0.0",
    "vite": "^4.5.0"
  }
}